{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "compat-api/html": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "border-image: fill 0 linear - gradient(#0001, #000)",
          "-webkit-tap-highlight-color"
        ]
      }
    ],
    "button-type": "off",
    "no-inline-styles": "off"
  }
}